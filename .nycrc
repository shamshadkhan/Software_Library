{
  "check-coverage": true,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50,
  "exclude": [
    "coverage/*",
    "node_modules/*",
    "divisionOfIntegers.js",
    "sum.js",
    "src/.internal/*"
   ],
   "reporter": [
     "lcov",
     "text-summary"
   ],
   "require": [],
  "extension": [
     ".js"
  ],
  "cache": false,
  "all": true,
  "report-dir": "./coverage"
}